{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.6.1",
  "default_locale": "en",
  "author": "Philipp Kewisch",
  "applications": {
    "gecko": {
      "id": "quickmove@mozilla.kewis.ch"
    }
  },
  "icons": {
    "32": "images/addon.png"
  },

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },

  "permissions": [
    "storage",
    "accountsRead",
    "messagesRead",
    "messagesWrite"
  ],

  "browser_action": {
    "default_icon": "images/addon.png",
    "default_popup": "popup/popup.html",
    "default_title": "__MSG_extensionName__",
    "browser_style": true
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "MacCtrl+Shift+M"
      }
    }
  }
}
